  function RetZero(ZEROS: string; QUANT: integer): string;
  function Maximo(nmCampo, nmTabela: String) : String;

var
  DM1: TDM1;
  pPADRAO, pPADRAO1,Situacao,
  CodEntrada, CodApartamento,
  Conexao, Apartamento  : string;

implementation

uses ModulodeDados;


{$R *.dfm}

function Maximo(nmCampo, nmTabela: String) : String;
begin
   with DM1.QRY0 do begin
      if Active then Close;
      SQL.Clear;
      SQL.ADD('SELECT MAX(' + NmCampo + ') FROM');
      SQL.ADD(NmTabela);
      Open;
      If Fields[0].AsString = '' Then
        result := RetZero('1',6)
      Else
        result := RetZero(IntToStr(Fields[0].AsInteger + 1),6);
      Close;
   end;
end;

function RetZero(ZEROS: string; QUANT: integer): string;
var
  I, Tamanho: integer;
  aux: string;
begin
  aux := zeros;
  Tamanho := length(ZEROS);
  ZEROS := '';
  for I := 1 to quant - tamanho do
    ZEROS := ZEROS + '0';
  aux := zeros + aux;
  RetZero := aux;
end;
procedure TDM1.DataModuleCreate(Sender: TObject);
var
  I: Integer;
begin
for I := 0 to Pred(Self.ComponentCount) do
 begin
   if Self.Components[I] is TSimpleDataSet then
   begin
     (Self.Components[I] as TSimpleDataSet).Connection:= DM.SQLC;
   end
   else
   if Self.Components[I] is TSQLQuery then
   begin
      (Self.Components[I] as TSQLQuery).SQLConnection:= DM.SQLC;
   end;
 end;
end;

end.
