Function TDM.LeIni(sArqIni : string; sSecao : string; sVariavel : string) : string;
var ArqIni  : tIniFile;
    sString : string;
begin
   ArqIni := tIniFile.Create( ExtractFilePath( Application.ExeName ) + '\' + sArqIni);
   Try
     result:= ArqIni.ReadString(sSecao, sVariavel, sString);
   Finally
     ArqIni.Free;
   end;
end;

